include Struture.pug
-let user =username
-let email = Myemail
-let ID = key
-let name=Fullname

    div(class=`container`)
        div(class='row card card-body border border-info m-3')
            div(class="col-12")
                div(class="form-group")
                label ID :
                input(type="text" value={ID} class="form-control" disabled)
            div(class="col-12")
                div(class="form-group")
                label Email : 
                input(type="text" value={email} class="form-control" disabled)
            div(class="col-12")
                div(class="form-group")
                label name : 
                input(type="text" value={name} class="form-control" disabled)
            div(class="col-12")
                div(class="form-group")
                label user : 
                input(type="text" value={user} class="form-control" disabled)
            div(class="col-12")
                div(class="form-group")
                    a(href="/logout" class="btn btn-danger m-3") Logout

